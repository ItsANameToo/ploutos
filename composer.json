{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/ArkEcosystem/php-crypto.git"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "arkecosystem/crypto": "dev-pubkeyhash",
        "arkx/ark-calculus": "^0.2.0",
        "doctrine/dbal": "^2.7",
        "fideloper/proxy": "^4.0",
        "graham-campbell/guzzle-factory": "^3.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "^1.0",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^3.7",
        "sentry/sentry-laravel": "^0.10.0",
        "shipping-docker/vessel": "^3.0",
        "spatie/flysystem-dropbox": "^1.0",
        "spatie/laravel-backup": "^5.10",
        "spatie/laravel-schemaless-attributes": "^1.1"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "~1.0",
        "codedungeon/phpunit-result-printer": "^0.19.12",
        "composer/composer": "^1.10",
        "filp/whoops": "^2.2",
        "fzaninotto/faker": "^1.8",
        "graham-campbell/analyzer": "^2.0",
        "laravel/dusk": "^3.0.0",
        "mockery/mockery": "^1.1",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.3",
        "themsaid/laravel-mail-preview": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "seed": [
            "php artisan migrate:fresh --seed"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
